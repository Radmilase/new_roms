<mujoco model="clock_only">

    <!-- ================= COMPILER / OPTIONS ================= -->
    <compiler
        angle="radian"
        coordinate="local"
        meshdir="mujoco_scanned_objects/models"/>

    <option timestep="0.002" iterations="20">
        <flag warmstart="enable"/>
    </option>

    <!-- ================= ASSETS ================= -->
    <asset>

        <!-- CLOCK VISUAL -->
        <mesh name="clock_visual"
              file="Crosley_Alarm_Clock_Vintage_Metal/model.obj"/>

        <!-- CLOCK COLLISION MESHES -->
        <mesh name="clock_collision_0"  file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_0.obj"/>
        <mesh name="clock_collision_1"  file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_1.obj"/>
        <mesh name="clock_collision_2"  file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_2.obj"/>
        <mesh name="clock_collision_3"  file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_3.obj"/>
        <mesh name="clock_collision_4"  file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_4.obj"/>
        <mesh name="clock_collision_5"  file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_5.obj"/>
        <mesh name="clock_collision_6"  file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_6.obj"/>
        <mesh name="clock_collision_7"  file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_7.obj"/>
        <mesh name="clock_collision_8"  file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_8.obj"/>
        <mesh name="clock_collision_9"  file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_9.obj"/>
        <mesh name="clock_collision_10" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_10.obj"/>
        <mesh name="clock_collision_11" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_11.obj"/>
        <mesh name="clock_collision_12" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_12.obj"/>
        <mesh name="clock_collision_13" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_13.obj"/>
        <mesh name="clock_collision_14" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_14.obj"/>
        <mesh name="clock_collision_15" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_15.obj"/>
        <mesh name="clock_collision_16" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_16.obj"/>
        <mesh name="clock_collision_17" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_17.obj"/>
        <mesh name="clock_collision_18" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_18.obj"/>
        <mesh name="clock_collision_19" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_19.obj"/>
        <mesh name="clock_collision_20" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_20.obj"/>
        <mesh name="clock_collision_21" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_21.obj"/>
        <mesh name="clock_collision_22" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_22.obj"/>
        <mesh name="clock_collision_23" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_23.obj"/>
        <mesh name="clock_collision_24" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_24.obj"/>
        <mesh name="clock_collision_25" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_25.obj"/>
        <mesh name="clock_collision_26" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_26.obj"/>
        <mesh name="clock_collision_27" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_27.obj"/>
        <mesh name="clock_collision_28" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_28.obj"/>
        <mesh name="clock_collision_29" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_29.obj"/>
        <mesh name="clock_collision_30" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_30.obj"/>
        <mesh name="clock_collision_31" file="Crosley_Alarm_Clock_Vintage_Metal/model_collision_31.obj"/>

        <!-- MATERIAL -->
        <material name="clock_material" rgba="0.9 0.9 0.9 1"/>

    </asset>

    <!-- ================= WORLD ================= -->
    <worldbody>

        <!-- FLOOR -->
        <geom name="floor"
              type="plane"
              pos="0 0 0"
              size="1 1 1"
              condim="3"/>

        <!-- CLOCK BODY -->
        <body name="object" pos="0 0 0.15">

            <!-- VISUAL -->
            <geom name="clock_visual"
                  type="mesh"
                  mesh="clock_visual"
                  material="clock_material"
                  contype="0"
                  conaffinity="0"/>

            <!-- COLLISION -->
            <geom mesh="clock_collision_0"  type="mesh" condim="6"/>
            <geom mesh="clock_collision_1"  type="mesh" condim="6"/>
            <geom mesh="clock_collision_2"  type="mesh" condim="6"/>
            <geom mesh="clock_collision_3"  type="mesh" condim="6"/>
            <geom mesh="clock_collision_4"  type="mesh" condim="6"/>
            <geom mesh="clock_collision_5"  type="mesh" condim="6"/>
            <geom mesh="clock_collision_6"  type="mesh" condim="6"/>
            <geom mesh="clock_collision_7"  type="mesh" condim="6"/>
            <geom mesh="clock_collision_8"  type="mesh" condim="6"/>
            <geom mesh="clock_collision_9"  type="mesh" condim="6"/>
            <geom mesh="clock_collision_10" type="mesh" condim="6"/>
            <geom mesh="clock_collision_11" type="mesh" condim="6"/>
            <geom mesh="clock_collision_12" type="mesh" condim="6"/>
            <geom mesh="clock_collision_13" type="mesh" condim="6"/>
            <geom mesh="clock_collision_14" type="mesh" condim="6"/>
            <geom mesh="clock_collision_15" type="mesh" condim="6"/>
            <geom mesh="clock_collision_16" type="mesh" condim="6"/>
            <geom mesh="clock_collision_17" type="mesh" condim="6"/>
            <geom mesh="clock_collision_18" type="mesh" condim="6"/>
            <geom mesh="clock_collision_19" type="mesh" condim="6"/>
            <geom mesh="clock_collision_20" type="mesh" condim="6"/>
            <geom mesh="clock_collision_21" type="mesh" condim="6"/>
            <geom mesh="clock_collision_22" type="mesh" condim="6"/>
            <geom mesh="clock_collision_23" type="mesh" condim="6"/>
            <geom mesh="clock_collision_24" type="mesh" condim="6"/>
            <geom mesh="clock_collision_25" type="mesh" condim="6"/>
            <geom mesh="clock_collision_26" type="mesh" condim="6"/>
            <geom mesh="clock_collision_27" type="mesh" condim="6"/>
            <geom mesh="clock_collision_28" type="mesh" condim="6"/>
            <geom mesh="clock_collision_29" type="mesh" condim="6"/>
            <geom mesh="clock_collision_30" type="mesh" condim="6"/>
            <geom mesh="clock_collision_31" type="mesh" condim="6"/>

            <!-- FREE JOINT -->
            <joint name="object:joint" type="free" damping="0.01"/>

            <site name="object:center" pos="0 0 0" size="0.01"/>

        </body>

        <!-- LIGHT -->
        <light name="light0"
               directional="true"
               pos="0 1 4"
               dir="0 0 -1"/>

        <!-- CAMERA -->
        <camera
            name="main_camera"
            mode="track"
            target="object"
            pos="1.00 1.35 0.60"
            quat="0.707 0 0.707 0"/>

    </worldbody>

</mujoco>
